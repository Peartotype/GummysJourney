            <string>if room == playmap instance_destroy();

if showkills
{
    if kills &lt; finalkills kills += scr_getstatspeed(kills, finalkills);
    killstext = round(kills)
}

if showbullets
{
    if bullets &lt; finalbullets bullets += scr_getstatspeed(bullets, finalbullets);
    bulletstext = round(bullets)
}

if showdamage
{
    if damage &lt; finaldamage damage += 0.1;
    damagetext = round(damage)
}

if showenergy
{
    if energy &lt; finalenergy energy += 0.1;
    energytext = round(energy)
}

if showscore
{
    if currentscore &lt; finalscore currentscore += scr_getstatspeed(currentscore, finalscore);
    scoretext = round(currentscore)
}
