{
    draw_set_colour(c_lime);
    draw_text(room_width/2, 128, "COMPLETE");
}

if showkills
{
    draw_set_colour(c_white);
    draw_text(room_width/3, 336, "Kills");
    draw_text((room_width/3)*2, 336, string(killstext));
}

if showbullets
{
    draw_set_colour(c_white);
    draw_text(room_width/3, 384, "Bullets fired");
    draw_set_colour(c_lime);
    draw_text((room_width/3)*2, 384, string(bulletstext));
}

if showdamage
{
    draw_set_colour(c_white);
    draw_text(room_width/3, 432, "Damage taken");
    draw_set_colour(c_red);
    draw_text((room_width/3)*2, 432, string(damagetext));
}

if showenergy
{
    draw_set_colour(c_white);
    draw_text(room_width/3, 480, "Energy used");
    draw_set_colour(c_yellow);
    draw_text((room_width/3)*2, 480, string(energytext));
}

if showscore
{
    draw_set_font(digitek);
    draw_set_colour(c_green);
    draw_text(room_width/2, 576, scoretext);
}

draw_set_colour(c_white);

