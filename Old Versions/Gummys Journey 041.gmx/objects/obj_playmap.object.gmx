{
    if (lid-1) &lt;= (global.maxlevel+1)
    {
        lid--;
        if lid &lt; 1 then lid = 1;
        if lid &gt; levellimit then lid = levellimit;
        event_user(1);
    }
    else
    {
        lid = levellimit;
    }
    sleft = false;
}

if sright 
{
    if (lid+1) &lt;= (global.maxlevel+1)
    {
        lid++;
        if lid &lt; 0 then lid = 0;
        if lid &gt; levellimit then lid = levellimit;
        event_user(1);
    }
    sright = false;
}

