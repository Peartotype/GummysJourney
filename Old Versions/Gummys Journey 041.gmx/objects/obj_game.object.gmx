        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Pause menu
if pause
{
    draw_sprite_stretched(spr_grey, 0, 0, 0, room_width, room_height);

    draw_set_font("healthfont");
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    draw_text(room_width/2, room_height/2, "PAUSED");
    draw_set_font("debugfont");
    draw_text(room_width/2, (room_height/2)+20, "Press ESC to continue");
    
    var pausebut1x = room_width/2-160;
    var pausebut1y = room_height/2+64;
    draw_sprite_ext(spr_button, pausebut1hover, pausebut1x, pausebut1y, 0.75, 0.75, 0, c_white, 1);
    draw_text(pausebut1x, pausebut1y, "Exit");
    
    var pausebut2x = room_width/2+160;
    var pausebut2y = room_height/2+64;
    draw_sprite_ext(spr_button, pausebut2hover, pausebut2x, pausebut2y, 0.75, 0.75, 0, c_white, 1);
    draw_text(pausebut2x, pausebut2y, "Restart");
}
else
{
    //Tutorial
    if tut
    {
        if director.mousecontrol and instance_exists(obj_tutmouse)
        {
            draw_set_font("debugfont");
            draw_set_halign(fa_center);
            draw_set_valign(fa_middle);
            draw_text( (view_xview[0]+view_wview[0])/2, (view_yview[0]+view_hview[0])/2, "OR");
            draw_sprite_stretched(spr_grey, 0, obj_tutmouse.x-32, obj_tutmouse.y-32, 204, 110);
        }
        
        if instance_exists(obj_tutdash)
        {
            draw_set_font("debugfont");
            draw_set_halign(fa_center);
            draw_set_valign(fa_middle);
            draw_text( (view_xview[0]+view_wview[0])/2, ((view_yview[0]+view_hview[0])/2)+32, "+");
            draw_sprite_stretched(spr_grey, 0, obj_tutdash.x-64, obj_tutdash.y-32, 128, 128)
        }
        //draw_sprite_stretched(spr_grey, 0, room_width/2-92, room_height/2-40, 204, 110);
        //draw_sprite_stretched(spr_grey, 0, room_width/2-92, room_height/2-40, 204, 110);
    }
    
    
    //HUD///////////////////////////////////////////////////////////////////////////////////////////////////
    ///////////////////////////////////////////////////////////////////////////////////////////////////////
    if global.drawhud
    {
        //Test Bottom Hud
        draw_sprite_stretched(spr_grey2, 0, 0, room_height-96, room_width, 96);
        
        //Healthbar///////////////////////////////
        scr_rect_col(healthbarx, healthbary, healthbarx+186, healthbary+22, c_black, true); //outline
        
        var hpPercent = obj_gummy.hp/obj_gummy.maxhp;
        if obj_gummy.hp &gt; 0 
        {
            //scr_rect_col(healthbarx+5, healthbary+3, (healthbarx+133)*hpPercent, healthbary+19, c_red, false); //healthbar
            //scr_rect_col(healthbarx+5, healthbary+16, (healthbarx+133)*hpPercent, healthbary+19, c_maroon, false); //lower shadow
            scr_rect_col(healthbarx+5, healthbary+3, (healthbarx+181)*hpPercent, healthbary+19, c_red, false); //healthbar
            scr_rect_col(healthbarx+5, healthbary+16, (healthbarx+181)*hpPercent, healthbary+19, c_maroon, false); //lower shadow
        }
        
        //draw_set_font("healthfont");
        draw_set_font("debugfont");
        draw_set_halign(fa_center);
        draw_set_valign(fa_middle);
        draw_set_color(c_black);
        draw_text( healthbarx+93, healthbary+13, string(obj_gummy.hp) + " / " + string(obj_gummy.maxhp));
        draw_set_color(c_white);
        
        /////////////////////////////////////////
        
        //Energy Bar/////////////////////////////
        scr_rect_col(energybarx, energybary, energybarx+186, energybary+22, c_black, true); //outline
        
        var energyPercent = obj_gummy.energy/obj_gummy.maxenergy;
        if obj_gummy.energy &gt; 0 
        {
            scr_rect_col(energybarx+5, energybary+3, (energybarx+181)*energyPercent, energybary+19, c_yellow, false); //energybar
            scr_rect_col(energybarx+5, energybary+16, (energybarx+181)*energyPercent, energybary+19, c_olive, false); //lower shadow
        }
    
        draw_set_font("debugfont");
        draw_set_halign(fa_center);
        draw_set_valign(fa_middle);
        draw_set_color(c_black);
        draw_text( energybarx+93, energybary+13, string(obj_gummy.energy) + " / " + string(obj_gummy.maxenergy));
        draw_set_color(c_white);
        
        /////////////////////////////////////////
        
        //Score//////////////////////////////////
        
        draw_set_font(digitek)
        draw_set_halign(fa_center);
        draw_set_valign(fa_middle);
        draw_set_colour(c_green);
        draw_text(room_width/2, room_height-72, obj_gummy.money);
        draw_set_colour(c_white);
        
        /////////////////////////////////////////
        
        if global.debug
        {
            draw_set_font("debugfont");
            
            //Debug Version
            var vstr = global.debugversion;
            var vx = view_xview[0]+view_wview[0]-string_width(vstr)
            var vy = view_yview[0]+view_hview[0]-20
            draw_set_halign(fa_left);
            draw_set_valign(fa_top);
            draw_set_font("debugfont");
            draw_text(vx, vy, vstr);
        
            //Debug display
            vstr = "DEBUG"
            vy = vy-20
            vx = view_xview[0]+view_wview[0]-string_width(vstr)
            draw_set_color(c_red);
            draw_text(vx, vy, vstr);
            draw_set_color(c_white);
            
        
            //Debug FPS
            vstr = string(fps)
            vy = view_yview[0]+40;
            vx = view_xview[0]+view_wview[0]-string_width(vstr)
            draw_set_color(c_green);
            draw_text(vx, vy, vstr);
            draw_set_color(c_white);
            
            //Debug wave
            if instance_exists(global.level) vstr = string(global.level.wave)
            else vstr = "END";
            vy = view_yview[0]+62;
            vx = view_xview[0]+view_wview[0]-string_width(vstr)
            draw_set_color(c_green);
            draw_text(vx, vy, vstr);
            draw_set_color(c_white);
        }
    }
}
