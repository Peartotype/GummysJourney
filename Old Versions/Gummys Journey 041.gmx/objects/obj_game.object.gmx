          <argument>
            <kind>1</kind>
            <string>randomize();
if introy &gt; 672
{
    obj_gummy.y = introy;
    introy -= introspeed;
    if background_vspeed[0] &lt; 7 then background_vspeed[0] += 0.05;
    if background_vspeed[1] &lt; 8 then background_vspeed[1] += 0.05;
}
else if intro
{
    audio_play_sound(battlemusic, 0, 1);
    instance_create(0, 0, global.level)
    if shouldtut = true
    {
        if director.mousecontrol
        {
            instance_create((room_width/2)+64, room_height/2, obj_tutspacebar);
            instance_create((room_width/2)-64, room_height/2, obj_tutmouse);
            instance_create(room_width/2, room_height/2+48, obj_tutarrows);
        }
        else
        {
            instance_create(room_width/2+64, room_height/2, obj_tutspacebar);
            instance_create(room_width/2-64, room_height/2, obj_tutarrows);
        }
        tut = true;
        alarm[0] = 60*7;
    }
    global.drawhud = true;
    intro = false;
}

global.enemycount = instance_number(ai_parent);

if pause
{
    if mouse_x &gt; pausebut1x-120 and mouse_x &lt; pausebut1x+120 and
        mouse_y &gt; pausebut1y-32 and mouse_y &lt; pausebut1y+32
    {
        pausebut1hover = 1;
    }
    else
    {
        pausebut1hover = 0;
    }
    
    if mouse_x &gt; pausebut2x-120 and mouse_x &lt; pausebut2x+120 and
        mouse_y &gt; pausebut2y-32 and mouse_y &lt; pausebut2y+32
    {
        pausebut2hover = 1;
    }
    else
    {
        pausebut2hover = 0;
    }
    
    if mouse_check_button_released(mb_left)
    {
        if pausebut1hover
        {
            pause = false;
            instance_activate_all();
            scr_stopbgrmusic();
            audio_play_sound(mainthemeslow, 0, 1);
            scr_fadeto(playmap);
        }
        
        if pausebut2hover
        {
            pause = false;
            instance_activate_all();
            scr_fadeto(room);
        }
    }
}

if keyboard_check_pressed(vk_escape)
{
    if !intro
    {
        if !pause
        {
            if obj_gummy.hp &lt; 1 
            {
                scr_fadeto(room);
            }
            else if instance_exists(obj_win)
            {
                instance_create(0, 0, obj_victorymenu);
                scr_stopbgrmusic();
                audio_play_sound(mainthemeslow, 0, 1);
                scr_fadeto(victorymenu);
            }
            else
            {
                if instance_exists(obj_tutspacebar) or instance_exists(obj_tutdash)
                {
                    with obj_tutpar
                    {
                        instance_destroy();
                    }
                    tut = false;
                }
                pause = true;
                instance_deactivate_all(true);
            }
        }
        else
        {
            pause = false;
            instance_activate_all();
        }
    }
    else
    {
        introspeed = 25;
        background_vspeed[0] = 7;
        background_vspeed[1] = 8;
    }
} 

</string>
